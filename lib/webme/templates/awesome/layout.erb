<HTML>
<HEAD>
  <TITLE><%= title %></TITLE>

  <META NAME="description" CONTENT="<%= description %>"> 

  <LINK rel="shortcut icon" href="assets/images/ruby.png"/>

  <LINK type="text/css" rel="stylesheet" href="assets/styles/reset.css" />
  <LINK type="text/css" rel="stylesheet" href="assets/styles/style.css" />

  <SCRIPT language="javascript" type="text/javascript" src="assets/scripts/jquery.js"></SCRIPT>
  <SCRIPT language="javascript" type="text/javascript" src="assets/scripts/highlight.js"></SCRIPT>

  <SCRIPT type="text/javascript">
    $(document).ready(function() {
      $('pre').wrapInner('<code></code>');
      hljs.tabReplace = '    ';
      hljs.initHighlightingOnLoad('ruby');
    });
  </SCRIPT>

  <LINK type="text/css" rel="stylesheet" href="assets/styles/highlight.css" />
</HEAD>
<BODY>

<div id="header">
<div class="centered">
  <div class="title">
    <h1>
      <img src="<%= logo %>" align="absmiddle" style="height: 60px;" />
      <%= title %>
    </h1>
  </div>
  <div class="menu">
    <a href="index.html">Home</a>  &nbsp;
    <a href="history.html">History</a> &nbsp;
    <a href="about.html">About</a> &nbsp;
    <a href="legal.html">Legal</a> &nbsp;
    <a href="logs.html">Logs</a> &nbsp;
    &middot; &nbsp;
    <% resources.each do |label, url| %>
      <% if url %><a href="<%= url %>"><%= label.capitalize %></a> &nbsp;<% end %>
    <% end %>
  </div>
</div>
</div>

<div class="main">
<div class="centered">
<%= content %>
</div>
</div>

<div id="footer">
<div class="centered">
  <div class="advert">
    <%= advert %>
  </div>
  <div class="copyright">
    <%= copyright %> <%= maintainer || authors.first %> |
    Website generated by <a href="http://proutils.github.com/webme">webme -t awesome</a><br/>
  </div>
</div>
</div>

</BODY>
</HTML>

