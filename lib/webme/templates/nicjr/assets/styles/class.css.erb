/* CLASS STYLES */

/*
  COLORS:
  Background: <%= colors.back %>
  Highlight : <%= colors.high %>
  Text Color: <%= colors.text %>
  Link Color: <%= colors.link %>
*/

.centered {
  width: 760px;
  margin: 0 auto;
}

.colorized {
  background: <%= colors.back %>;
  color: <%= colors.text %>;
}

.highlighted {
  background: <%= colors.back %>;
  color: <%= colors.high %>;
}

