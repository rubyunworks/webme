<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01//EN"
   "http://www.w3.org/TR/html4/strict.dtd">

<HTML>
<HEAD>
  <TITLE><%= title %></TITLE>

  <LINK rel="shortcut icon" href="assets/ruby.png"/>

  <LINK type="text/css" rel="stylesheet" href="assets/reset.css" />
  <LINK type="text/css" rel="stylesheet" href="assets/custom.css" />

  <SCRIPT language="javascript" type="text/javascript" src="assets/jquery.js"></SCRIPT>
  <SCRIPT language="javascript" type="text/javascript" src="assets/highlight.js"></SCRIPT>

  <SCRIPT type="text/javascript">
    $(document).ready(function() {
      $('pre').wrapInner('<code></code>');
      hljs.tabReplace = '    ';
      hljs.initHighlightingOnLoad('ruby');
    });
  </SCRIPT>

  <LINK type="text/css" rel="stylesheet" href="assets/highlight.css" />
</HEAD>
<BODY>

<div id="header">
  <div class="title"> 
    <% if logo %>
      <div class="logo">
        <a href="<%= link %>"><img src="<%= logo %>" /></a>
      </div>    
    <% else %>
      <div class="download">
        Get Version
        <h1><a href="<%= link %>"><%= version %></a></h1>
      </div>
    <% end %>
    <%= header %>
  </div>
</div>

<div id="nav">
  <div class="menu">
    <ul>
    <li><a href="#" style="font-size: 175%;"><%= title %></a></li>
    <!-- TODO: javascript TOC -->
    </ul>
  </div>
</div>

<div id="main">
  <div class="doc">
    <%= body %>
  </div>
</div>

<div id="footer">
<div class="advert">
<%= advert %>
</div>
  <div class="copyright">
    <%= copyright %> | generated by <a href="http://proutils.github.com/webme">webme</a> -t tink<br/>
  </div>
</div>

</BODY>
</HTML>

